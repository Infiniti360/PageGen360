{
  "name": "universal-pom-generator",
  "version": "1.0.0",
  "description": "A comprehensive, AI-enhanced Page Object Model (POM) generator that supports multiple testing frameworks and languages for any website",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist generated-pom generated-pom-enhanced",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:examples": "jest examples",
    "test:comprehensive": "node tests/run-comprehensive-tests.js",
    "test:monitoring": "jest tests/real-time-monitoring-suite.ts",
    "test:cross-platform": "jest tests/cross-platform-suite.ts",
    "test:coverage": "jest --coverage --verbose",
    "test:watch": "jest --watch",
    "test:debug": "jest --debug",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "docs": "typedoc src/index.ts --out docs/api",
    "generate": "node scripts/cli/cli-universal.js",
    "generate-enhanced": "node scripts/cli/cli-universal-enhanced.js",
    "generate-interactive": "node scripts/cli/cli-universal-interactive.js",
    "example:basic": "node examples/basic/universal-examples.js",
    "example:advanced": "node examples/advanced/",
    "example:auth": "node examples/authentication/"
  },
  "keywords": [
    "pom",
    "page-object-model",
    "test-automation",
    "playwright",
    "selenium",
    "cypress",
    "puppeteer",
    "testcafe",
    "typescript",
    "javascript",
    "python",
    "java",
    "csharp",
    "ai",
    "openai",
    "universal",
    "generator"
  ],
  "author": "Universal POM Generator Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/universal-pom-generator.git"
  },
  "bugs": {
    "url": "https://github.com/your-repo/universal-pom-generator/issues"
  },
  "homepage": "https://github.com/your-repo/universal-pom-generator#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.0",
    "inquirer": "^9.2.0",
    "openai": "^4.20.0",
    "playwright": "^1.40.0",
    "puppeteer": "^21.5.0",
    "selenium-webdriver": "^4.15.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.8.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "dist/**/*",
    "scripts/**/*",
    "docs/**/*",
    "examples/**/*",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "pom-generator": "./scripts/cli/cli-universal.js",
    "pom-generator-enhanced": "./scripts/cli/cli-universal-enhanced.js"
  }
}
